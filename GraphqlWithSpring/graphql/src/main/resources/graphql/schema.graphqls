type Query{
    authors: [Author]
    authorById(id: ID!): Author
}

type Author {
    id: ID!
    name: String!
    books: [Book]
}

type Book{
    id: ID!
    title: String!
    name: String
}

type Mutation{
  addBook(bookInput: BookInput): Book
}

input BookInput{
 authorId: ID!
 name: String!
 title: String
}
